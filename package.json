  "license": "ISC",
  "bugs": {
    "url": "https://github.com/onetech-project/kongko/issues"
  },
  "homepage": "https://github.com/onetech-project/kongko#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-redis": "^5.1.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-socket.io-session": "^1.3.5",
    "minify": "^7.0.1",
    "moment": "^2.29.1",
    "redis": "^3.0.2",
    "socket.io": "^3.1.2"
